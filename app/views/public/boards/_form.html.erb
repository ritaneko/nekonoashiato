   <div class="container">
      <%= form_with model: board, local: true do |f| %>
        <div class="form-group">
           <label>タイトル</label>
             <%= f.text_field :title, class: "form-control" %>
        </div>
        <div class="form-group">
           <label>相談内容</label>
             <%= f.text_area :body, class: "form-control", rows: 10 %>
        </div>
 
        <%= f.submit '投稿', class: "btn btn-lg btn-block btn-primary mt-4" %>
      <% end %>
  </div>
  