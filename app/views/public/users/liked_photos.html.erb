<% content_for(:title, 'いいね一覧') %>

<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h4 class="headline mt-5 ml-5">いいねした投稿</h4>
      <div class="row mt-3">
        <% @liked_photos.each do |photo| %>
          <%= render 'public/photos/photo', photo: photo %>
        <% end %>
        <div class="d-flex justify-content-center">
         <%= paginate @liked_photos %>
        </div>
      </div>
    </div>
  </div>
</div>