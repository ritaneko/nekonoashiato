<h3>タグリスト</h3>
<% @tag_list.each do |list| %>
  <%= link_to list.tag_name, tag_photos_path(tag_id: list.id) %>
  <%= "(#{list.photos.count})" %>
<% end %>
<br>
 <%= "#{@tag.tag_name}" %>
<br>

<% @photos.each do |photo| %>
  <%= link_to photo.body, photo %>
<% end %>

<%= form_with model: @photo do |f| %>
 <div>
    <%= f.label"タグ名(スペースで区切ると複数登録できます。)" %>
    <%= f.text_field :size => 80 %> 
 </div>
 
 <div>
   <%= f.label"タグ" %>
   <%= f.text_field :tag_name, value: @tag_list %>
 </div>
   <%= f.submit "編集する" %>
<% end %>
