<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
  <% if @user == current_user %>
  <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
  <% end %>
</div>

<% @photos.each do |photo| %>
<div>
  <%= link_to photo_path(photo.id) do %>
   <%= image_tag photo.get_image %>
  <% end %>
  <p>ユーザーアイコン:<%= image_tag photo.user.get_profile_image(100,100) %></p>
  <p>タイトル:<%= photo.title %></p>
  <p>コメント:<%= photo.body %></p>
  <p>ユーザーネーム:<%= photo.user.name %></p>
</div>
<% end %>